execute pathogen#infect()

set nocompatible

set nu
set ruler
set encoding=utf-8
set wrap
set tabstop=4
set shiftwidth=4
set expandtab
set cursorcolumn
set cursorline
set autoindent

syntax enable
set background=dark
let g:solarized_termcolors=256
colorscheme solarized

let g:airline_theme='deus'

nmap <F8> :TagbarToggle<CR>

